#include <GL/glut.h>
#include <math.h>
#include <vector>

GLuint texture[1];
double angle = 0;
typedef struct{
	int x;
	int y;
	int z;

	double u;
	double v;
}Vertices;

const double PI = math.PI;

void drawSphere(float radius, unsigned int rings, unsigned int sectors){

	glClear(GL_COLOR_BUFFER_BIT);

	glFlush();
}

void draw(){
	drawSphere(2, 5, 10);
}

int main(int argc, char** argv){
	glutInit(&argc, argv);
	glutCreateWindow("simple");
	glutDisplayFunc(draw);
	glutMainLoop();
}
